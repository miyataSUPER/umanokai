{
  "version": 2,
  "name": "uma-no-kai-odd-logic",
  "rewrites": [
    {
      "source": "/api/odds",
      "destination": "/api/odds.py"
    },
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/odds.py": {
      "maxDuration": 60
    }
  }
}
