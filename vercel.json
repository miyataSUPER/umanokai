{
  "version": 2,
  "name": "uma-no-kai-odd-logic",
  "rewrites": [
    {
      "source": "/api/odds",
      "destination": "/api/odds.py"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ],
  "functions": {
    "api/odds.py": {
      "maxDuration": 60
    }
  },
  "outputDirectory": "public"
}
